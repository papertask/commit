<?xml version="1.0" encoding="utf-8"?><!--<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"--><!--android:layout_width="match_parent"--><!--android:layout_height="wrap_content">-->

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <View
                android:id="@+id/header_space"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="20dp"
                android:background="#D8D8D8"
                android:visibility="gone" />

            <com.joooonho.SelectableRoundedImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/image_avatar"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_below="@+id/header_space"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:scaleType="centerCrop"
                android:src="@drawable/icon_w"
                app:sriv_left_bottom_corner_radius="48dip"
                app:sriv_left_top_corner_radius="16dip"
                app:sriv_oval="true"
                app:sriv_right_bottom_corner_radius="16dip"
                app:sriv_right_top_corner_radius="0dip"

                />

            <TextView
                android:id="@+id/tv_name"
                android:layout_width="fill_parent"
                android:layout_height="39dp"
                android:layout_below="@+id/image_avatar"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="#FFFF"
                android:editable="false"
                android:gravity="center"
                android:imeOptions="actionDone"
                android:maxLines="1"
                android:paddingLeft="10dp"
                android:paddingRight="8dp"
                android:text="Nguyen A"
                android:textColor="@color/colorPrimary"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/tv_location"
                android:layout_width="fill_parent"
                android:layout_height="39dp"
                android:layout_below="@id/tv_name"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="#FFFF"
                android:drawableLeft="@drawable/infor_location"
                android:drawablePadding="5dp"
                android:editable="false"
                android:gravity="center_vertical"
                android:imeOptions="actionDone"
                android:maxLines="1"
                android:paddingLeft="10dp"
                android:paddingRight="8dp"
                android:text="text1"
                android:textColor="#000"
                android:textSize="16sp" />


            <TextView
                android:id="@+id/tv_interviewDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_location"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:drawableLeft="@drawable/info_date"
                android:drawablePadding="5dp"
                android:editable="false"
                android:padding="10dp"
                android:text="text2"
                android:textColor="#000"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/tv_gender"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_interviewDate"
                android:layout_marginLeft="13dp"
                android:drawableLeft="@drawable/flag"
                android:drawablePadding="5dp"
                android:padding="10dp"

                android:paddingLeft="10dp"
                android:text=""
                android:textColor="#000"
                android:textSize="16sp" />

            <LinearLayout
                android:id="@+id/ln_action_profile"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_gender"
                android:layout_marginBottom="3dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:paddingLeft="1.5dp"
                android:paddingRight="1.5dp"
                android:weightSum="2">

                <Button
                    android:id="@+id/btn_qdetail_ma_delete"
                    android:layout_width="0dp"
                    android:layout_height="36dp"
                    android:layout_marginRight="3dp"
                    android:layout_weight="1"
                    android:background="@drawable/buttoncustom"
                    android:text="删除"
                    android:textColor="@color/White"
                    android:textSize="16sp" />

                <Button
                    android:id="@+id/btn_qdetail_ma_share"
                    android:layout_width="0dp"
                    android:layout_height="36dp"
                    android:layout_marginLeft="3dp"
                    android:layout_weight="1"
                    android:background="@drawable/buttoncustom_white"
                    android:text="分享"
                    android:textColor="@color/colorPrimary"
                    android:textSize="16sp" />
            </LinearLayout>

            <View
                android:id="@+id/space_bottom_1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/ln_action_profile"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="20dp"
                android:background="#D8D8D8" />

            <LinearLayout
                android:id="@+id/ln_result_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/space_bottom_1"
                android:layout_marginLeft="15dp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:editable="false"
                    android:text="答卷详情"
                    android:textSize="20sp" />

                <View
                    android:layout_width="30dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="10dp"
                    android:background="@color/colorPrimary" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/ln_anounce"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ln_result_1"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_play_anounce"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_gravity="center_horizontal"
                    android:clickable="true"
                    android:src="@drawable/oval_play" />

                <TextView
                    android:id="@+id/lbl_result"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:editable="false"
                    android:text="同意办证:"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_result_cn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:editable="false"
                    android:text="谢谢你的配合"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tv_result"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="60dp"
                    android:layout_marginTop="10dp"
                    android:editable="false"
                    android:text="Dong y"
                    android:textSize="18sp" />

                <ImageView
                    android:id="@+id/img_play_anounce_1"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_gravity="center_horizontal"
                    android:clickable="true"
                    android:src="@drawable/oval_play"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/lbl_result_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:editable="false"
                    android:text="同意办证:"
                    android:textSize="20sp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_result_cn_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:editable="false"
                    android:text="谢谢你的配合"
                    android:textSize="18sp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_result_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="60dp"
                    android:layout_marginTop="10dp"
                    android:text="Dong y"
                    android:textSize="18sp"
                    android:visibility="gone" />

                <Button
                    android:id="@+id/btn_marrage_translate"
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/buttoncustom_white"
                    android:text="联系翻译"
                    android:textColor="@color/colorPrimary"
                    android:textSize="16sp" />

                <Button
                    android:id="@+id/btn_marrage_section"
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/buttoncustom_white"
                    android:text="联系有关部门"
                    android:textColor="@color/colorPrimary"
                    android:textSize="16sp" />

                <Button
                    android:id="@+id/btn_marrage_embassy"
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginBottom="50dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/buttoncustom_white"
                    android:text="联系使馆"
                    android:textColor="@color/colorPrimary"
                    android:textSize="16sp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ln_result_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/ln_anounce"
                android:layout_marginLeft="15dp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:editable="false"
                    android:text="问卷"
                    android:textSize="20sp" />

                <View
                    android:layout_width="30dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="10dp"
                    android:background="@color/colorPrimary" />
            </LinearLayout>


            <com.interview.iso.view.ExpandableHeightListView
                android:id="@+id/list_questionnaire"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ln_result_2"
                android:divider="@null"
                android:padding="15dp" />

            <TextView
                android:id="@+id/nonolege"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/list_questionnaire"
                android:layout_marginTop="15dp"
                android:editable="false"
                android:padding="15dp"
                android:text="建议："
                android:textColor="#000"
                android:textSize="22sp"
                android:visibility="gone" />

            <TextView
                android:id="@+id/textnologe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/nonolege"
                android:editable="false"
                android:paddingLeft="25dp"
                android:paddingRight="15dp"
                android:text="通知公安进一步筛查并播放“疑似拐卖”的录音告知申请人"
                android:visibility="gone" />

            <TextView
                android:id="@+id/textnologe1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textnologe"
                android:editable="false"
                android:paddingBottom="10dp"
                android:paddingLeft="25dp"
                android:paddingRight="15dp"
                android:paddingTop="10dp"
                android:text="播放“同意结婚申请”的录音 "
                android:visibility="gone" />

            <RelativeLayout
                android:id="@+id/extra_info_header"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_below="@+id/textnologe1"
                android:background="#ededed"
                android:visibility="gone">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="10dp"
                    android:text="结果告知：Results informing"
                    android:textColor="#000"
                    android:textSize="18sp"
                    android:textStyle="bold" />
            </RelativeLayout>

            <TextView
                android:id="@+id/extra_information"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/extra_info_header"
                android:editable="false"
                android:gravity="left"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:paddingTop="25dp"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp" />

        </RelativeLayout>

    </ScrollView>
</RelativeLayout>